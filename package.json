{
  "name": "tagasaurus",
  "version": "0.0.2",
  "description": "Tag the Planet!",
  "author": "Alexander V. Mantzaris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mantzaris/Tagasaurus.git"
  },
  "homepage": "https://github.com/mantzaris/Tagasaurus#readme",
  "bugs": {
    "url": "https://github.com/mantzaris/Tagasaurus/issues"
  },
  "keywords": [
    "tagging",
    "documenter",
    "photos",
    "comments",
    "emoticons",
    "sentiment",
    "hastags"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^18.0.0",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "electron-reload": "^1.5.0",
    "electron-reloader": "^1.2.0"
  },
  "dependencies": {
    "better-sqlite3": "^7.5.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "install": "^0.13.0",
    "json": "^11.0.0",
    "masonry-layout": "^4.2.2",
    "path": "^0.12.7"
  },
  "selfUpdate": false,
  "build": {
    "name": "tagasaurus",
    "description": "tag your world",
    "homepage": "http://github.com/mantzaris/tagasaurus",
    "license": "MIT",
    "author": "mantzaris https://twitter.com/Tagasaurus_app",
    "appId": "tagasaurus",
    "repository": {
      "type": "git",
      "url": "http://github.com/mantzaris/tagasaurus"
    },
    "files": [
      "!.github",
      "!README.md",
      "!dist/"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "taga-icon/TagaIcon512x512.png",
      "maintainer": "mantzaris",
      "category": "utility"
    },
    "portable": {
      "artifactName": "tagasaurus.app"
    }
  },
  "postinstall": "electron-builder install-app-deps"
}
