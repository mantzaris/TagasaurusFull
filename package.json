{
  "name": "tagasaurus",
  "version": "1.3.0",
  "description": "Tag your Planet!",
  "author": "Alexander V. Mantzaris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mantzaris/Tagasaurus.git"
  },
  "homepage": "https://github.com/mantzaris/Tagasaurus#readme",
  "bugs": {
    "url": "https://github.com/mantzaris/Tagasaurus/issues"
  },
  "keywords": [
    "tagging",
    "documenter",
    "photos",
    "comments",
    "emoticons",
    "sentiment",
    "hastags"
  ],
  "main": "main.js",
  "postinstall": "electron-builder install-app-deps",
  "scripts": {
    "start": "npm run start-dev --build_installer=false",
    "clean": "rm -rf ./node_modules && rm -rf ./dist",
    "rebuild": "npm run clean && npm install && npm rebuild && electron-rebuild",
    "start-win": "npm run start-dev --build_installer=true",
    "start-dev": "electron  .",
    "build": "electron-builder $OSTARGETS",
    "build-win-portable": "electron-builder --win=zip",
    "build-win-installer": "electron-builder --win=nsis",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "electron": "^18.0.0",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "electron-reload": "^1.5.0",
    "electron-reloader": "^1.2.0",
    "prettier": "2.8.1"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.1",
    "@tensorflow/tfjs-node": "^4.1.0",
    "@vladmandic/face-api": "^1.7.10",
    "better-sqlite3": "^7.6.2",
    "decode-gif": "^1.0.1",
    "dotenv": "^16.0.3",
    "electron-pdf-window": "^1.0.12",
    "file-type": "16.5.3",
    "fs-extra": "^10.0.0",
    "json": "^11.0.0",
    "masonry-layout": "^4.2.2",
    "path": "^0.12.7"
  },
  "selfUpdate": false,
  "build": {
    "appId": "tagasaurus",
    "files": [
      "!.github",
      "!README.md",
      "!docs"
    ],
    "linux": {
      "icon": "taga-icon/TagaIcon512x512.png",
      "maintainer": "mantzaris",
      "category": "utility"
    },
    "win": {
      "icon": "taga-icon/TagaIcon512x512.png"
    },
    "portable": {
      "artifactName": "tagasaurus.app"
    }
  }
}
